{
  "name": "@hollon-ai/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared types and utilities for Hollon-AI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.ts\""
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
